{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e ../../.env.local -- next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "prisma-dev": "prisma generate && dotenv -e ../../.env.local -- prisma db pull",
    "prisma-migrate": "dotenv -e ../../.env.local -- pnpx prisma migrate dev",
    "prisma-studio": "dotenv -e ../../.env.local -- pnpx prisma studio",
    "prisma-push": "dotenv -e ../../.env.local -- prisma db push",
    "prisma-prod": "prisma generate && dotenv -e .env.prod -- prisma db pull",
    "vercel-build": "prisma generate && next build"
  },
  "dependencies": {
    "@/ui": "workspace:*",
    "@auth/prisma-adapter": "^2.4.1",
    "@hookform/resolvers": "^3.9.0",
    "@next/env": "^14.2.4",
    "@prisma/client": "5.17.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@stripe/stripe-js": "^4.1.0",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.416.0",
    "next": "14.2.5",
    "next-auth": "5.0.0-beta.19",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "react-icons": "^5.2.1",
    "recharts": "^2.12.7",
    "stripe": "^11.6.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/next": "^9.0.0",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prisma": "^5.16.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3"
  }
}
