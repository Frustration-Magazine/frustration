{
  "name": "web",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "dotenv -e ../../.env -- astro dev",
    "start": "dotenv -e ../../.env -- astro dev",
    "build": "dotenv -e ../../.env -- astro build",
    "preview": "dotenv -e ../../.env -- astro preview",
    "astro": "dotenv -e ../../.env -- astro",
    "lint": "eslint .",
    "stripe-webhook": "dotenv -e ../../.env -- stripe listen --forward-to http://localhost:4321/api/webhook",
    "vercel-prisma": "cd ../../ && pnpm dlx prisma generate --schema='./packages/data-access/prisma/schema.prisma' && pnpm dlx prisma db push --schema='./packages/data-access/prisma/schema.prisma' --accept-data-loss && cd -",
    "vercel-build": "pnpm run vercel-prisma && eslint . && astro build"
  },
  "dependencies": {
    "@/data-access": "workspace:*",
    "@/ui": "workspace:*",
    "@astrojs/react": "^4.1.2",
    "@astrojs/tailwind": "^5.1.4",
    "@astrojs/vercel": "^8.0.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/bakbak-one": "^5.1.1",
    "@fontsource/bebas-neue": "^5.1.1",
    "@fontsource/montserrat": "^5.1.1",
    "@fontsource/open-sans": "^5.1.1",
    "@fontsource/roboto": "^5.1.1",
    "@mui/material": "^6.3.0",
    "@radix-ui/react-progress": "1.1.1",
    "@splidejs/splide": "^4.1.4",
    "@splidejs/splide-extension-auto-scroll": "^0.5.3",
    "@splidejs/splide-extension-video": "^0.8.0",
    "@stripe/react-stripe-js": "^3.1.1",
    "@stripe/stripe-js": "^5.4.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@vercel/analytics": "^1.4.1",
    "@vercel/speed-insights": "^1.1.0",
    "astro": "^5.1.2",
    "balloons-js": "^0.0.2",
    "clsx": "^2.1.1",
    "framer-motion": "^11.15.0",
    "lucide-react": "^0.469.0",
    "puppeteer": "^23.11.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.4.0",
    "react-router-dom": "^7.1.1",
    "rough-notation": "^0.5.1",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "^1.10.4",
    "@eslint/js": "^9.17.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "daisyui": "^4.12.23",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "stripe": "^17.5.0",
    "typescript": "^5.7.2"
  }
}
