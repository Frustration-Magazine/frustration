    "daisyui": "^4.12.13",
    "dotenv": "^16.4.5",
    "eslint": "^9.9.0",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "stripe": "^16.9.0",
    "typescript": "^5.5.4"
  }
