{
  "name": "web",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "dotenv -e ../../.env -- astro dev",
    "start": "dotenv -e ../../.env -- astro dev",
    "build": "dotenv -e ../../.env -- astro build",
    "preview": "dotenv -e ../../.env -- astro preview",
    "astro": "dotenv -e ../../.env -- astro",
    "lint": "eslint .",
    "stripe-webhook": "dotenv -e ../../.env -- stripe listen --forward-to http://localhost:4321/api/webhook",
    "vercel-build": "cd ../../ && pnpm dlx prisma generate --schema='./packages/data-access/prisma/schema.prisma' && pnpm dlx prisma db push --schema='./packages/data-access/prisma/schema.prisma' --accept-data-loss && cd - && eslint . && astro build"
  },
  "dependencies": {
    "@/data-access": "workspace:*",
    "@/ui": "workspace:*",
    "@astrojs/react": "^3.6.2",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/vercel": "^7.8.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/bakbak-one": "^5.1.0",
    "@fontsource/bebas-neue": "^5.0.21",
    "@fontsource/montserrat": "^5.1.0",
    "@fontsource/open-sans": "^5.0.29",
    "@fontsource/roboto": "^5.1.0",
    "@mui/material": "^6.1.5",
    "@radix-ui/react-progress": "1.1.0",
    "@splidejs/splide": "^4.1.4",
    "@splidejs/splide-extension-auto-scroll": "^0.5.3",
    "@splidejs/splide-extension-video": "^0.8.0",
    "@stripe/react-stripe-js": "^2.8.0",
    "@stripe/stripe-js": "^4.4.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.12",
    "astro": "^4.15.2",
    "clsx": "^2.1.1",
    "framer-motion": "^11.11.11",
    "lucide-react": "^0.429.0",
    "puppeteer": "^23.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.2",
    "rough-notation": "^0.5.1",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.10"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "^1.10.1",
