{
  "name": "web",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "infisical run -- astro dev",
    "start": "infisical run --env prod -- astro dev",
    "build": "infisical run -- astro build",
    "preview": "infisical run -- astro preview",
