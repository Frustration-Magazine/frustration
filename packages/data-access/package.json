{
  "name": "data-access",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "prisma generate",
    "generate": "prisma generate",
    "reset": "infisical run -- prisma migrate reset",
    "migrate": "infisical run -- prisma migrate dev",
    "migrate:dev": "infisical run --env=dev -- prisma migrate deploy",
    "migrate:prod": "infisical run --env=prod -- prisma migrate deploy",
    "pull": "infisical run -- prisma db pull && prisma generate",
    "pull:prod": "infisical run --env=prod -- prisma db pull && prisma generate",
    "push": "infisical run -- prisma db push",
    "studio": "infisical run -- prisma studio",
    "studio:prod": "infisical run --env=prod -- prisma studio"
  },
  "keywords": [],
  "author": "Thibaut Izard <thibaut.izard@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@stripe/stripe-js": "^7.9.0",
    "puppeteer": "^24.20.0",
    "resend": "^4.8.0",
    "stripe": "^18.5.0",
    "utils": "workspace:*"
  },
  "devDependencies": {
    "@prisma/client": "^6.16.1",
    "@types/node": "^22.18.1",
    "prisma": "^6.16.1"
  },
  "exports": {
    "./prisma": "./prisma/client.ts",
    "./*": "./*.ts"
  }
}
